digraph {
	rankdir=LR size="10,5"
	node [fillcolor=lightgrey fontsize=12 shape=box style="rounded, filled"]
	SES [label="Socioeconomic Status"]
	LF [label="Lifestyle Factors (Diet, Exercise)"]
	SW [label="Shift Work" color=red]
	CD [label="Circadian Disruption"]
	HI [label="Hormonal Imbalance"]
	CR [label="Cancer Risk"]
	SES -> SW
	SES -> LF
	LF -> CR
	SW -> CD
	CD -> HI
	HI -> CR
	LF -> CD
}
